Print 7 stars using do while loop: 
*******
*******
*******
*******
*******
*******
*******
*******

#include<iostream>

using namespace std;

int main()
{

int i=1, j=1;

	do
	{
		do
		{
			cout << "*";

			j++;
		} 
		while (j<=7);
		j = 1;
		cout << endl;
		i++;
	}
	while (i<=8);
  
  
	return 0;
}
