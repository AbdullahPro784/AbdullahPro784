#include<iostream>
//primt prime numbers 2 to 100;
using namespace std;
int main()
{
	int count=0;
	int i = 2,j=1;
	while(i <= 100)
	{

		while(j <= i)
		{
			if (i%j == 0)
			{
				count++;
			}
			j++;
		}
		j = 1;
		if (count == 2)
		{
			cout << i << "Number is prime! "<<endl;
		}
		count = 0;
		i++;
	}
	
	return 0;
}
