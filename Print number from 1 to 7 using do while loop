	#include<iostream>

using namespace std;

int main()
{

	int i=1, j=1;
	do
	{
		
		do
		{
			cout <<j;
			j++;
		}
		while (j<=7);
		cout << endl;
		j = 1;

		i++;
	}
    while (i<=8);
    
    
	return 0;
}
